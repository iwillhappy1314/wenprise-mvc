{
  "name": "wenprise/mvc",
  "description": "wenprise mvc framework.",
  "keywords": [
    "wenprise",
    "framework",
    "WordPress"
  ],
  "authors": [
    {
      "name": "Amos Lee",
      "email": "iwillhappy1314@gmail.com",
      "homepage": "https://www.wpzhiku.com/"
    }
  ],
  "autoload": {
    "psr-4": {
      "Wenprise\\": "src/"
    },
    "files": [
      "src/helpers.php",
      "src/Wenprise.php"
    ]
  },
  "require": {
    "php": ">=5.6.4",
    "composer/installers": "^1.0",
    "illuminate/routing": "~5.5.0",
    "illuminate/container": "~5.5.0",
    "illuminate/http": "~5.5.0",
    "illuminate/events": "~5.5.0",
    "illuminate/view": "~5.5.0",
    "illuminate/filesystem": "~5.5.0",
    "illuminate/database": "~5.5.0",
    "knplabs/knp-menu": "^2.2",
    "plasticbrain/php-flash-messages": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "5.3.*",
    "mockery/mockery": "^0.9.4"
  },
  "minimum-stability": "stable",
  "config": {
    "preferred-install": {
      "*": "dist"
    }
  }
}
